{
	"name": "vscode-autohotkey-plus",
	"displayName": "AutoHotkey",
	"description": "AutoHotkey language support for VS Code",
	"version": "1.0.0",
	"publisher": "cweijan",
	"engines": {
		"vscode": "^1.30.0"
	},
	"activationEvents": [
		"*"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "icon.png",
	"bugs": {
		"url": "https://github.com/cweijan/vscode-autohotkey/issues"
	},
	"homepage": "https://github.com/cweijan/vscode-autohotkey/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/cweijan/vscode-autohotkey.git"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"keywords": [
		"vscode",
		"autohotkey",
		"ahk"
	],
	"contributes": {
		"languages": [
			{
				"id": "ahk",
				"aliases": [
					"AutoHotkey",
					"autohotkey",
					"ahk"
				],
				"extensions": [
					".ahk",
					".ext"
				],
				"configuration": "./ahk.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ahk",
				"scopeName": "source.ahk",
				"path": "./syntaxes/ahk.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "ahk",
				"path": "./snippets/ahk.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"tslint": "tslint -t verbose src/**/*.ts"
	},
	"dependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.15.0",
		"typescript": "^3.4.3"
	},
	"devDependencies": {
		"vscode": "^1.1.34"
	}
}
